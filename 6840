#include <stdio.h>

int main(void){
    int a,b,c;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    if(a>b){
        if(a>c){
            if(b>c)
                printf("%d",b);
            else
                printf("%d",c);
        }
    }
    if(a<b){
        if(b>c){
            if(a>c)
                printf("%d",a);
            else
                printf("%d",c);
        }
    }
    if(c>b){
        if(c>a){
            if(a>b)
                printf("%d",a);
            else
                printf("%d",b);
        }
    }
    return 0;
}
