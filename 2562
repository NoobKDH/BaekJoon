#include <stdio.h>
#include <stdlib.h>

int main(void){
    int a[9];
    int max = 0;
    int b;
    
    for(int i = 0; i<9;i++){
        scanf("%d",&a[i]);
    }
    for(int i =0; i<9;i++){
        if(max<a[i]){
            max = a[i];
            b = i;
        }
    }
    printf("%d\n", max);
    printf("%d", b+1);
    
    return 0;
    
}
