#include <stdio.h>
#include <string.h>
int main() {
    int n, r;
    char c[20];
    
    scanf("%d", &n);  
    
    for(int i=0; i<n; i++) {   
        scanf("%d %s", &r, c);   
        
        for(int j=0; j<strlen(c); j++) {  
            for(int k=0; k<r; k++)  
                printf("%c", c[j]);
        }
        printf("\n");  
    }
}
