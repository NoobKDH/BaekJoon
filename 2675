#include <stdio.h>
#include <string.h>

int main(void){
    int n,r;
    char c[20];
    scanf("%d", &n);
    for(int i =0; i<n; i++){

        scanf("%d %s", &r,c);
        for(int k =0; k<strlen(c); k++){
            for(int j =0; j<r; j++)
                printf("%c", c[k]);
        }
        printf("\n");
    }
}
