#include <stdio.h>

int main(void){
    int n,m,i,j,k;
    int a[100] ={0};
    
    scanf("%d %d",&n,&m);
    
    for(int x =1; x<=m;x++){
        scanf("%d %d %d", &i, &j, &k);
        for(int y=i;y<=j;y++){
            a[y] = k;
        }
    }
    
    for(int x = 1; x<=n; x++){
        printf("%d ", a[x]);
    }
    
    return 0;
}
