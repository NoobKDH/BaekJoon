#include <stdio.h>

int main(void){
    int n;
    int a[100];
    int x,ct=0;
    scanf("%d", &n);
    for(int i =0;i<n; i++){
        scanf("%d",&a[i]);
    }
    scanf("%d", &x);
    for(int i =0; i<n;i++){
        if(a[i]==x)
            ct +=1;
    }
    printf("%d",ct);
    
    return 0;
    
}
