#include <stdio.h>

int main(void){
    int n;
    scanf("%d",&n);
    int a[n];
    int min = 1000000, max =-1000000;
    for(int i =0; i<n;i++){
        scanf("%d",&a[i]);
        if(max<a[i])
            max = a[i];
        if(min>a[i])
            min = a[i];
    }
    printf("%d %d", min, max);
}
